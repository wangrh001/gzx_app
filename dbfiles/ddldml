-- ----------------------------
--  Procedure structure for `accounting`
-- ----------------------------
DROP PROCEDURE IF EXISTS `accounting`;
delimiter ;;
CREATE DEFINER = `root`@`localhost` PROCEDURE `accounting`()
LANGUAGE SQL
DETERMINISTIC
CONTAINS SQL
SQL SECURITY DEFINER
COMMENT ''
begin
	declare a,b INT;
	declare done boolean default 0;
	#declare b,c VARCHAR;
	DECLARE mycursor CURSOR FOR SELECT id from gzx.cfg_cbs_estate_area;
	declare continue handler for sqlstate '02000' set done=1;
	OPEN mycursor;

	repeat
	FETCH mycursor INTO a;
	update cfg_cbs_estate_area set city=2 where id=a;
	
	until done end repeat;

	CLOSE mycursor;
END
 ;;
delimiter ;

